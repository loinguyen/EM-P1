.minilist-btn{
	position: relative;
	height: 2em;
	width: 350px;
}
.minilist-btn div{
	float: left;
	margin-left: 10px;
	margin-top: 0.1em;
/* 	width: 103px; */
	width: 159px;
	height: 1.9em;
	background-color: #595959;
	cursor: pointer;
	text-align: center;
	color: #FFF;
}.minilist-btn div span{
	position: relative;
	top: 3px;
}
div .active{
	background-color: #019875;
}
.minilist-btn div:hover{
	box-shadow: inset 0px 0px 0px 2px #019875;
	color: #019875;
	background: #FFF;
}
#search-result{
	position: absolute;
	margin-top: 50px;
	width: 330px;
/* 	height: 50%; */
	background: #FFF;
	border: 1px solid #d2d2d2;
	box-shadow: 0px 5px 5px #000;
	z-index: 999;
}
#search-result div .mCSB_scrollTools {
	right: 1px;
}
#bookmark-bar{
	width:0px;
	height:95px;
	background-color:rgba(0,0,0,0.3);
	float:left;
	overflow: auto;
	margin-left: -50px;
}
.bookmark-item{
	position:relative;
	height:70px;
	width:170px;
	float:left;
	margin-top: 10px;
	margin-left: 15px;
	color: #FFF;
	cursor: pointer;
    -moz-box-shadow: inset 0 0 5px #555;
    -webkit-box-shadow: inset 0 0 5px #555;
    box-shadow: inset 0 0 5px #555;
}
span.bm-title{
	height: 100%;
	width: 95%;
	cursor: pointer;
	padding: 0.5em;
	color: white;
}
.bm-title a {
	text-decoration: none;
	color: white;
}
.notification-icon {
	position: absolute;
	z-index: 9999;
	top: -10px;
	right: -10px;
}
.bookmark-notification {
	display:none;
	position: absolute;
	left:-10000px;
	top:0;
	width:170px;
/* 	height:70px; */
	background:#595959;
    -moz-box-shadow: inset 0 0 5px #555;
    -webkit-box-shadow: inset 0 0 5px #555;
    box-shadow: inset 0 0 5px #555;
}
.bookmark-notification a{
	display: inline-block;
	padding: 0.5em;
}
 #expander{
	float: left;
	height: 100px;
	width: 52px;
} 
#category-expander {
	background-image: url("../images/categoryIcon/all.png");
	background-repeat: no-repeat;
	height: 44px;
	width: 44px;
	background-size: 44px;
	float: left;
	cursor: pointer;
	margin-left: 4px;
	margin-top: 4px;
}
#category-expander:hover{
	margin-top:2px;
}
#category-expander:active{
	margin-top:0;
}
#category-selector {
	display:none;
	position: absolute;
	margin-top: 50px;
	background: #d2d2d2;
	width: 355px;
	left: 0;
	z-index: 999;
}
#category-selector > span {
	float: left;
	font-size: 13pt;
	padding: 10px 15px 10px;
}
#category-selector > a {
	float: left;
	width: 170px;
	padding: 5px 10px 5px;
	cursor: pointer;
}
#category-selector > a:hover {
	background:#019875;
	color:#FFF;
}
#category-selector > a:active {
	background:#FFF;
	color:#019875;
}
#category-selector > a > img {
	width: 44px;
	float: left;
}
#category-selector > a > label {
	font-size: 12pt;
	float: left;
	padding-top: 12px;
	padding-left: 15px;
	cursor: pointer;
}
#date-expander {
	background-image: url("../images/dateIcon/Week.png");
	background-repeat: no-repeat;
	height: 44px;
	width: 44px;
	background-size: 44px;
	cursor: pointer;
	margin-left: 4px;
	margin-top: 50px;
}
#date-expander:hover{
	margin-top:48px;
}
#date-expander:active{
	margin-top:46px;
}
#date-selector{
	display:none;
	position: absolute;
/* 	background: rgba(0,0,0,0); */
	margin-top: 100px;
	left: 355px;
	width: 60px;
	border: 1px solid rgba(0,0,0,0.5);
   border-radius: 5px 5px 2px 2px;
   background: rgba(0,0,0,0.25);
