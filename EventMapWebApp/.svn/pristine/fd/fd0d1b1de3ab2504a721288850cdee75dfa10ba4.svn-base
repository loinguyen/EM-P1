#full-page{
	position:fixed;
	left:0px;
	top:0px;
	width:100%;
	height:100%;
}
#header{
	z-index: 999;
	position: relative;
	height: 40px;
	background-color: black;
}
#content{
	position:absolute;
	z-index:0;			
	width:100%;
	height:100%;
	display: fixed;
	margin: 40px auto;
}
#map-canvas{
	position:absolute;
	height:100%;
	width:100%;
	overflow: hidden;
	right: 0px;
	top: 0px;
}
#wrapper{
	background-color:rgba(0,0,0,0.3);
	width:355px;
	height: 100%;
	overflow: hidden;
	float:left;
}
#expand-btn {
	background: url("../images/homepage/btn-subscribe-notice.png");
	width: 26px;
	height: 26px;
	float:left;
	cursor: pointer;
	z-index: 0;
}
#expand-btn.collapsed {
	margin-left: -50px;
}

#expand-btn.new {
	background: url("../images/homepage/btn-subscribe-notice-new.png");
}
#expand-btn label{
	position: relative;
	top: -0.1em;
	font-size: 14pt;
	left: 0.45em;
	cursor: pointer;
}
#expand-btn.new label{
	color: red;
}
#expand-btn:hover{
}
#wrapper-rpl{
	z-index:-1;
	width:355px;
	height: 100%;
}
#sidebar{
	width:376px;
	height:100%;
	z-index:2;
	overflow:auto;
	bottom: 50px;
}
img {
	border: none !important;
}
html, body{
	margin: 0;
	padding: 0;
	height: 100%;
}
#map-canvas {
	margin: 0;
	height: 100%;
	z-index: -1;	
}
#search-box {
	width: 320px;
	margin-top: .5em;
	margin-left: .54em;
}
#search-input {
	border: 1px solid #d2d2d2;
	height: 44px;
	padding-left: 10px;
	float: left;
}
div.item {
	overflow: hidden;
	display: inline-block;
	position:relative;
	opacity:1!important;
	height: 150px;
	width: 330px;
	background-color: #FFF;
	margin: .5em 0.58em;
	cursor:pointer;
}
span.info {
	position: absolute;
	width: 100%;
	background: rgba(0,0,0,.6);
	color: white;
	
	-webkit-transition: all .25s ease-out;
	-moz-transition: all .25s ease-out;
	-ms-transition: all .25s ease-out;
	-o-transition: all .25s ease-out;
	transition: all .25s ease-out;
	
}
.info label {
	position: absolute;
	margin-left: 1em;
	width: 80%;
	cursor:pointer;
}
.info span {
	margin-right: 1em;
	float: right;
}
.info .inforwindow {
	margin-top: 1.2em;
}
.info .title {
	margin-top: .5em
}
.info .time, .location {
	margin-top: 1em
}
/* REVEAL UP FULL */
div.item.revealUpFull span { 
	height: 150px;
	bottom: -100px;
}

div.item.revealUpFull:hover span {
	bottom: 0px;
}

div.item.revealUpInit span { 
	height: 150px;
	bottom: 0px;
}
div.sub-btn {
	position: absolute;
	cursor: pointer;
	bottom: 7px;
	left: 287px;
	height:36px;
	width:36px;
	z-index: 99;
}
div.sub-unclicked {
	background:url("../images/homepage/btn-subscribe-small.jpg");
	background-position:36px 0;
}
div.sub-clicked {
	background:url("../images/homepage/btn-subscribe-small.jpg");
	background-position:0 0;
}
div.sub-btn:hover{
	box-shadow: inset 0px 0px 5px #FFF;
}
#colapse-btn{
	background-color: #019875;
	background-image: url("../images/homepage/updownarrow.png");
	background-size:30px 10px;
	background-repeat:no-repeat;
	background-position:center; 
	width: 355px;
	height: 15px;
	cursor: pointer;
}
#colapse-btn.down {
	transform:rotate(180deg);
	-ms-transform:rotate(180deg); /* IE 9 */
	-webkit-transform:rotate(180deg); /* Safari and Chrome */
}
#colapse-btn:click{
}
