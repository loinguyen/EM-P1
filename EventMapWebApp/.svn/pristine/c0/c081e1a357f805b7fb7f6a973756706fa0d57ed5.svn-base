<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Bản đồ sự kiện | EventMap</title>
<meta property="og:title" content="Bản đồ sự kiện | EventMap" />
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="width=device-width, initial-scale=1.0"> 

<link rel="stylesheet" type="text/css" href="<%=getServletContext().getContextPath()%>/css/listEvents.css" />
<link rel="stylesheet" type="text/css" href="<%=getServletContext().getContextPath()%>/css/style.css" />
<!-- Tab Category -->
<link rel="stylesheet" type="text/css" href="<%=getServletContext().getContextPath()%>/css/category.css" />
<!-- Custom scrollbars CSS -->
<link href="<%=getServletContext().getContextPath()%>/css/jquery.mCustomScrollbar.css" rel="stylesheet" type="text/css" />

<!-- Common and page js -->
<script src="<%=getServletContext().getContextPath()%>/js/jquery-2.0.2.js"></script>
<script src="<%=getServletContext().getContextPath()%>/js/jquery.mCustomScrollbar.js"></script>
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/js/modernizr.custom.97074.js"></script>
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/js/listEvents.js"></script>
<!-- Direction aware -->
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/js/jquery.hoverdir.js"></script>
<!-- Page easing -->
<script type="text/javascript" src="<%=getServletContext().getContextPath()%>/js/jquery.easing.1.3.js"></script>
</head>
<body>
<div id="header">
	<jsp:include page="/WEB-INF/page/Header.jsp" />
</div>
<div id="loadingImg"></div>
<div id="nav">
	<div class="nav-box">
		<ul>
			<li><a href="#music" data-slide="1" category="Âm nhạc">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/music.png)"></i>
					<span>Âm nhạc</span>
				</a></li><li><a href="#workshop" data-slide="0" category="Hội thảo">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/fair.png)"></i>
					<span>Hội thảo</span>
				</a></li>
			<li><a href="#sport" data-slide="0"  category="Thể thao">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/sport.png)"></i>
					<span>Thể thao</span>
				</a></li>
			<li><a href="#art" data-slide="0"  category="Nghệ thuật">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/art.png)"></i>
					<span>Nghệ thuật</span>
				</a></li>
			<li><a href="#film" data-slide="0"  category="Điện ảnh">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/film.png)"></i>
					<span>Điện ảnh</span>
				</a></li>
			<li><a href="#meeting" data-slide="0"  category="Họp mặt">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/offline.png)"></i>
					<span>Họp mặt</span>
				</a></li>
			<li><a href="#promotion" data-slide="0"  category="Khuyến mại">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/saleoff.png)"></i>
					<span>Khuyến mại</span>
				</a></li>
			<li><a href="#others" data-slide="0"  category="Khác">
					<i class="dot"></i>
					<i class="icon" style="background-image: url(<%=getServletContext().getContextPath()%>/images/categoryIcon/other.png)"></i>
					<span>Khác</span>
				</a></li>
		</ul>
	</div>
</div>
<div class="container" id="music"></div>
<div class="container" id="workshop"></div>
<div class="container" id="sport"></div>
<div class="container" id="art"></div>
<div class="container" id="film"></div>
<div class="container" id="meeting"></div>
<div class="container" id="promotion"></div>
<div class="container" id="others"></div>
</body>
</html>